{
  "name": "laneros-item-filter",
  "version": "1.0.0",
  "description": "Chrome extension to filter sold items on laneros.com",
  "main": "content.js",
  "scripts": {
    "generate-icons": "node scripts/generate-icons.js",
    "test": "jest --testPathIgnorePatterns=.*\\.e2e\\.test\\.js$",
    "test:watch": "jest --watch --testPathIgnorePatterns=.*\\.e2e\\.test\\.js$",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:all": "npm run test && npm run test:e2e",
    "pack": "zip -r extension.zip . -x \"node_modules/*\" \".git/*\" \".github/*\" \"*.zip\""
  },
  "keywords": [
    "chrome-extension",
    "laneros",
    "filter",
    "marketplace"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "sharp": "^0.33.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^22.0.0"
  }
} 